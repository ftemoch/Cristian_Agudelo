@model IEnumerable<Clase12.Models.Movie>
@{
    ViewBag.Title = "Peliculas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Peliculas</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Peliculas</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
            <tr>
                <td>@movie.Nombre</td>
            </tr>
        }
    </tbody>
</table>

