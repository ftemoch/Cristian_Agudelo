@model IEnumerable<Clase12.Models.Cliente>
@{
    ViewBag.Title = "Lista";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<h2>Lista</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Clientes</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cliente in Model)
        {
            <tr>
                <td>@cliente.Nombre</td>
            </tr>
        }
    </tbody>
</table>


